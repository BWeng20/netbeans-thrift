<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>    
    <fontcolor name="keyword"           foreColor="blue"   default="default"/>
    <fontcolor name="number"            foreColor="red"    default="default"/>    
    <fontcolor name="identifier"        foreColor="yellow" default="default"/>
    <fontcolor name="comment"           foreColor="gray"   default="default"/>
    <fontcolor name="separator"         foreColor="black"  default="default"/>
    <fontcolor name="whitespace"        foreColor="black"  default="default"/>    
    <fontcolor name="default"           foreColor="black"  default="default"/>    
    
</fontscolors>
